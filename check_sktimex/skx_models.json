{
    "#-- constant --": {},

    "skx.mean": {
        "class": "sktimex.forecasting.ConstantForecaster",
        "mode": "mean"
    },
    "skx.median": {
        "class": "sktimex.forecasting.ConstantForecaster",
        "mode": "median"
    },

    "#-- reducers --": {},

    "skx.reducer_lr": {
        "class": "sktimex.forecasting.ReducerForecaster",
        "estimator": {
            "class": "sklearn.linear_model.LinearRegression"
        },
        "strategy": "recursive",
        "window_length": 24,
        "prediction_length": 6
    },
    "skx.regressor_lin": {
        "class": "sktimex.forecasting.RegressorForecaster",
        "estimator": "sklearn.linear_model.LinearRegression",
        "lags": [24,24,0],
        "tlags": 6
    },
    "skx.sklearn_lin": {
        "class": "sktimex.forecasting.ScikitLearnForecaster",
        "estimator": "sklearn.linear_model.LinearRegression",
        "window_length": 24,
        "prediction_length": 6
    }
}