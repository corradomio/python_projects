{
    "#-- constant --": {},

    "skx.MeanForecaster": {
        "class": "sktimex.forecasting.ConstantForecaster",
        "mode": "mean"
    },
    "skx.MedianForecaster": {
        "class": "sktimex.forecasting.ConstantForecaster",
        "mode": "median"
    },

    "#-- reducers --": {},

    "skx.reducer.LinearRegression": {
        "class": "sktimex.forecasting.ReducerForecaster",
        "estimator": {
            "class": "sklearn.linear_model.LinearRegression"
        },
        "strategy": "recursive",
        "window_length": 24,
        "prediction_length": 6
    },
    "skx.regressor.LinearRegression": {
        "class": "sktimex.forecasting.RegressorForecaster",
        "estimator": {
            "class": "sklearn.linear_model.LinearRegression"
        },
        "lags": [24,24,0],
        "tlags": 6
    },
    "skx.sklearn.LinearRegression": {
        "class": "sktimex.forecasting.ScikitLearnForecaster",
        "estimator": {
            "class": "sklearn.linear_model.LinearRegression"
        },
        "window_length": 24,
        "prediction_length": 6
    }
}